          <groundspeak:text encoded="False">Found quickly, left Ace of Spades playing card!</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
  </wpt>
