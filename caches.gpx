    <groundspeak:cache id="585218" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Watermills Chimney</groundspeak:name>
      <groundspeak:placed_by>browni</groundspeak:placed_by>
      <groundspeak:owner id="306873">browni</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
      <groundspeak:difficulty>3</groundspeak:difficulty>
