    <groundspeak:cache id="1240029" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>This Son went to Keele.</groundspeak:name>
      <groundspeak:placed_by>Lost'n'Wet</groundspeak:placed_by>
      <groundspeak:owner id="1447876">lostnwet</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
