          <groundspeak:text encoded="False">Found with keiman, hokeycokey and debbeh! Thanks for the cache.</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
  </wpt>
