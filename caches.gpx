          <groundspeak:finder id="2391193">dordotky</groundspeak:finder>
          <groundspeak:text encoded="False">Wonderful idea and lovely location to walk to!</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
  </wpt>
