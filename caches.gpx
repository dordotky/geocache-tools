    <groundspeak:cache id="1861366" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Cara's Cache</groundspeak:name>
      <groundspeak:placed_by>rebelrebelzs</groundspeak:placed_by>
      <groundspeak:owner id="3231190">rebelrebelzs</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
