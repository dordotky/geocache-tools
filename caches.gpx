    <groundspeak:cache id="1800324" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Dwalin - Eccleshall Ring</groundspeak:name>
      <groundspeak:placed_by>sharant</groundspeak:placed_by>
