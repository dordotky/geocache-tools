          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="2391193">dordotky</groundspeak:finder>
          <groundspeak:text encoded="False">Found tonight with debbeh, keiman and hokeycokey! Thanks for the cache!</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
