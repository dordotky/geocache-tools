        <groundspeak:log id="126107643">
          <groundspeak:date>2010-09-09T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="2391193">dordotky</groundspeak:finder>
          <groundspeak:text encoded="False">Didn't take long to find, not too sure of the area. Thanks for the cache!</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
