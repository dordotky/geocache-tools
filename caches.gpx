    <name>GC2CTNW</name>
    <desc>Staffs Union 1 by VCasambros, Traditional Cache (1.5/1.5)</desc>
    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=55f70ccb-0615-4c9c-8006-63eaed88de35</url>
    <urlname>Staffs Union 1</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="1818117" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Staffs Union 1</groundspeak:name>
      <groundspeak:placed_by>VCasambros</groundspeak:placed_by>
      <groundspeak:owner id="3331436">VCasambros</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
